{
    "name": "sensiolabs-de/deptrac",
    "description": "Deptrac is a static code analysis tool that helps to enforce rules for dependencies between software layers.",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "graphp/graphviz": "0.2.*",
        "nikic/php-parser": "^4.1",
        "phpdocumentor/graphviz": "~1.0",
        "symfony/config": "^4.1",
        "symfony/console": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/event-dispatcher": "^4.1",
        "symfony/finder": "^4.1",
        "symfony/options-resolver": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "authors": [
        {
            "name": "timglabisch",
            "email": "tim.glabisch@sensiolabs.de"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "SensioLabs\\Deptrac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SensioLabs\\Deptrac\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
