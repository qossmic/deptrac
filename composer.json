{
    "name": "sensiolabs-de/deptrac",
    "description": "Deptrac is a static code analysis tool that helps to enforce rules for dependencies between software layers.",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "graphp/graphviz": "0.2.*",
        "nikic/php-parser": "^4.0",
        "padraic/phar-updater": "^1.0",
        "phpdocumentor/graphviz": "~1.0",
        "sensiolabs-de/astrunner": "dev-master",
        "symfony/config": "^3.4",
        "symfony/console": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/event-dispatcher": "^3.4",
        "symfony/filesystem": "^3.4",
        "symfony/finder": "^3.4",
        "symfony/options-resolver": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "authors": [
        {
            "name": "timglabisch",
            "email": "tim.glabisch@sensiolabs.de"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "SensioLabs\\Deptrac\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SensioLabs\\Deptrac\\": "tests/"
        }
    }
}
