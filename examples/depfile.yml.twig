{% import _self as dep %}

{% macro some_models(name, value, type, size) %}
type: className
regex: .*MyNamespace\\Models\\.*
{% endmacro %}

paths: ["./examples/ModelController1"]
exclude_files: []
layers:
- name: Models
  collectors:
    - {{ dep.some_models('some_models') | yaml }}
    - {{ include('some-include.depfile.yml.twig') | yaml }}
  layers:
    - name: ModelsA
      collectors: []
- name: Controller
  collectors:
    - type: className
      regex: .*MyNamespace\\.*Controller.*
ruleset: []